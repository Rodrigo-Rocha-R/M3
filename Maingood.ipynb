    "def agent_portrayal(agent):\n",
    "    size = 20\n",
    "    color = \"tab:red\"\n",
    "    shape = \"circle\"\n",
    "    \n",
    "    # Verificar el tipo de agente\n",
    "    if isinstance(agent, CarAgent):\n",
    "        size = 50\n",
    "        color = \"tab:blue\"\n",
    "        shape = \"circle\"\n",
    "    elif isinstance(agent, TrafficLightAgent):\n",
    "        size = 20\n",
    "        shape = \"circle\"\n",
    "        if agent.state == 1:\n",
    "            color = \"tab:red\"\n",
    "        elif agent.state == 2:\n",
    "            color = \"tab:green\"\n",
    "        elif agent.state == 3:  \n",
    "            color = \"yellow\"  \n",
    "    return {\"size\": size, \"color\": color, \"shape\": shape}\n",
    "\n",
    "\n"
